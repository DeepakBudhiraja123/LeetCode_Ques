class Solution {
public:
    vector<int> twoSum(vector<int>& nums, int target) {
        vector<int> res;
        for(int i = 0; i<nums.size()-1; i++){
            int num1 = nums[i];
            for(int j = i+1; j<nums.size(); j++){
                int num2 = nums[j];
                if(num1 + num2 == target){
                    res.push_back(i);
                    res.push_back(j);
                    return res;
                }
            }
        }
        return res;
    }
};
